{
  "$schema": ".rnv/schema/rnv.project.json",
  "extendsTemplate": "@rnv/template-starter/renative.json",
  "projectName": "hello-renative",
  "workspaceID": "rnv",
  "templateConfig": {
    "name": "@rnv/template-starter",
    "version": "1.7.0-rc.0"
  },
  "defaults": {
    "supportedPlatforms": [
      "web",
      "ios",
      "android",
      "androidtv",
      "firetv",
      "tvos",
      "macos",
      "linux",
      "windows",
      "tizen",
      "webos",
      "chromecast",
      "kaios",
      "androidwear",
      "tizenwatch"
    ]
  },
  "plugins": {
    "react-redux": {
      "version": "7.2.8",
      "webpackConfig": {
        "moduleAliases": true,
        "modulePaths": true
      }
    },
    "@reduxjs/toolkit": "source:rnv",
    "@react-native-async-storage/async-storage": "source:rnv",
    "redux": {
      "version": "^5.0.1",
      "webpackConfig": {
        "moduleAliases": true,
        "modulePaths": true
      }
    },

    "react-native-paper": {
      "version": "5.12.5",
      "webpackConfig": {
        "modulePaths": true,
        "moduleAliases": true
      }
    },
    "react-native-vector-icons": "source:rnv",
    "react-native-safe-area-context": "source:rnv"
  }
}
